a=int(input("enter first no"))
b=int(input("enter second no"))
print("print 1 for plus")
print("print 2 for minuse")
print("print 3 for divide")
print("print 4 for multiply")
c=int(input("choose any one"))
if(c==1):
    print(a+b)
elif(c==2):
    print(a-b)
elif(c==3):
    print(a//b)
if(c==4):
    print(a*b)
